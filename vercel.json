{
  "crons": [
    {
      "path": "/api/cron/sync-wallet-stats",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/log-balance",
      "schedule": "5 0 * * *"
    },
    {
      "path": "/api/trending-tokens",
      "schedule": "*/30 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/__/auth/(.*)",
      "destination": "https://dplug-687cf.firebaseapp.com/__/auth/$1"
    },
    {
      "source": "/__/firebase/init.json",
      "destination": "/api/firebase-handler?init=1"
    },
    {
      "source": "/api/firebase-config",
      "destination": "/api/firebase-handler?config=1"
    },
    {
      "source": "/api/profile/followers-count",
      "destination": "/api/profile-handler?action=followers-count"
    },
    {
      "source": "/api/profile/followers-list",
      "destination": "/api/profile-handler?action=followers-list"
    },
    {
      "source": "/api/profile/by-handle",
      "destination": "/api/profile-handler?action=by-handle"
    },
    {
      "source": "/api/watchlist/add",
      "destination": "/api/watchlist-handler?action=add"
    },
    {
      "source": "/api/watchlist/remove",
      "destination": "/api/watchlist-handler?action=remove"
    },
    {
      "source": "/api/watchlist/sync-index",
      "destination": "/api/watchlist-handler?action=sync-index"
    },
    {
      "source": "/api/push/register",
      "destination": "/api/push-handler?action=register"
    },
    {
      "source": "/api/push/send",
      "destination": "/api/push-handler?action=send"
    },
    {
      "source": "/api/push/status",
      "destination": "/api/push-handler?action=status"
    },
    {
      "source": "/((?!api/|__/|\\.well-known).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        },
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    },
    {
      "source": "/workbox-:hash.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.jpg",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:path*.jpeg",
      "headers": [
        {
